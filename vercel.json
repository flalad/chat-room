{
  "version": 2,
  "name": "private-chat-room",
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    }
  ],
  "functions": {
    "api/server.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}